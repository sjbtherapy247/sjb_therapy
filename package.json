{
  "name": "sjb-therapy",
  "author": "TezD",
  "version": "2.0.0",
  "description": "SJB-Therapy-website",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5002",
    "ssl": "next dev --experimental-https",
    "devssl": "node scripts/next-dev-ssl.js & next dev -p 5002",
    "start": "next start",
    "build": "next build",
    "postbuild": "next-sitemap --config=next-sitemap.config.js",
    "build-netlify": "next build && cp -r .next _next && mv _next .next/",
    "export": "next build && next export -o _static",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "prettier": "prettier --write 'src/**/*.{js,jsx}'",
    "clear-all": "rm -rf .next _static node_modules",
    "re-start": "rm -rf .next _static node_modules && npm install && npm run dev",
    "re-build": "rm -rf .next _static node_modules && npm install && npm run build"
  },
  "dependencies": {
